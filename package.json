{
  "name": "radiant-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --config .config/build/vite.config.production.ts",
    "build:analyze": "npm run build:prod && open dist/stats.html",
    "build:optimize": "node scripts/build-optimize.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fsd": "eslint . --config .eslintrc.fsd.cjs",
    "typecheck": "tsc --noEmit",
    "fsd:check": "npx steiger ./src",
    "fsd:fix": "npx steiger ./src --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --config .config/testing/vitest.config.integration.ts",
    "test:integration:coverage": "vitest --config .config/testing/vitest.config.integration.ts --coverage",
    "test:unit": "vitest 'src/**/*.{test,spec}.{ts,tsx,js,jsx}' --exclude 'src/**/*.integration.test.*'",
    "test:components": "vitest 'src/**/*component*/**/*.{test,spec}.{ts,tsx,js,jsx}' 'src/**/*component*/**/__tests__/**/*.{ts,tsx,js,jsx}'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:visual": "playwright test --config tests/visual/visual-regression.config.ts",
    "test:visual:update": "playwright test --config tests/visual/visual-regression.config.ts --update-snapshots",
    "test:mobile": "playwright test --config .config/testing/playwright.config.mobile.ts",
    "test:mobile:ui": "playwright test --config .config/testing/playwright.config.mobile.ts --ui",
    "test:mobile:debug": "playwright test --config .config/testing/playwright.config.mobile.ts --debug",
    "test:mobile:responsive": "playwright test tests/mobile/responsive.spec.ts --config .config/testing/playwright.config.mobile.ts",
    "test:mobile:performance": "playwright test tests/mobile/performance.spec.ts --config .config/testing/playwright.config.mobile.ts",
    "test:mobile:visual": "playwright test tests/mobile/visual.spec.ts --config .config/testing/playwright.config.mobile.ts",
    "test:mobile:visual:update": "playwright test tests/mobile/visual.spec.ts --config .config/testing/playwright.config.mobile.ts --update-snapshots",
    "test:mobile:a11y": "playwright test tests/mobile/accessibility.spec.ts --config .config/testing/playwright.config.mobile.ts",
    "security:audit": "node scripts/security-audit.cjs",
    "security:fix": "npm audit fix",
    "security:fix:force": "npm audit fix --force",
    "perf:analyze": "node scripts/bundle-analyzer.cjs",
    "perf:analyze:save": "node scripts/bundle-analyzer.cjs --save-baseline",
    "perf:test": "playwright test --config .config/testing/playwright.performance.config.ts",
    "perf:test:update": "playwright test --config .config/testing/playwright.performance.config.ts --update-snapshots",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:mobile",
    "test:ci": "npm run test:coverage && npm run test:integration:coverage && npm run test:e2e && npm run test:mobile",
    "test:menu": "node scripts/test-automation.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "commit": "cz",
    "prepare": "husky",
    "deploy": "node scripts/deploy.js",
    "deploy:vercel": "node scripts/deploy.js vercel",
    "deploy:netlify": "node scripts/deploy.js netlify",
    "deploy:aws": "node scripts/deploy.js aws",
    "deploy:cloudflare": "node scripts/deploy.js cloudflare",
    "deploy:staging": "node scripts/deploy.js vercel staging"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/react": "^9.44.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.534.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-resizable-panels": "^3.0.4",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.9.0",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "steiger": "^0.5.9",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "tests/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

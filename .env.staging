# Staging Environment Configuration
# Copy this file and update with your staging values

# Environment
VITE_APP_ENV=staging
VITE_APP_VERSION=1.0.0-staging

# API Configuration
VITE_API_URL=https://staging-api.example.com
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_DOMAIN=staging-auth.example.com
VITE_AUTH_CLIENT_ID=your-staging-auth-client-id
VITE_AUTH_REDIRECT_URI=https://staging.example.com/callback

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_PWA=false

# Third-party Services
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
VITE_GA_TRACKING_ID=G-STAGING-ID
VITE_PLAUSIBLE_DOMAIN=staging.example.com

# Public URLs
VITE_PUBLIC_URL=https://staging.example.com
VITE_CDN_URL=

# Build Info (set by CI/CD)
VITE_BUILD_TIMESTAMP=
VITE_COMMIT_SHA=